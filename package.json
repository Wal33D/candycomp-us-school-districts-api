{
  "name": "us-school-districts-api",
  "version": "0.1.6",
  "description": "High-performance, memory-optimized API for US school district lookups using official NCES boundary data",
  "main": "dist/server.js",
  "bin": {
    "school-district-cli": "./dist/cli.js"
  },
  "esModuleInterop": true,
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,json}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,json}'",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist coverage logs",
    "prepare": "husky install && npm run build || true",
    "cli": "ts-node src/cli.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts,tsx",
    "ignore": [
      "dist/**/*"
    ]
  },
  "license": "MIT",
  "author": "Waleed Judah <waleed@glitchgaming.us>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wal33D/us-school-districts-api.git"
  },
  "bugs": {
    "url": "https://github.com/Wal33D/us-school-districts-api/issues"
  },
  "homepage": "https://github.com/Wal33D/us-school-districts-api#readme",
  "keywords": [
    "api",
    "school-districts",
    "geospatial",
    "education",
    "nces",
    "gis",
    "nodejs",
    "typescript",
    "express",
    "rest-api"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@turf/bbox": "^7.2.0",
    "@turf/boolean-point-in-polygon": "^7.2.0",
    "@turf/simplify": "^7.2.0",
    "@turf/turf": "^7.2.0",
    "@types/compression": "^1.8.1",
    "commander": "^14.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "node-fetch": "2.7.0",
    "rbush": "^3.0.1",
    "shapefile": "^0.6.6",
    "unzipper": "^0.12.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.4.0",
    "@types/node-fetch": "^2.6.12",
    "@types/rbush": "^4.0.0",
    "@types/shapefile": "^0.6.4",
    "@types/supertest": "^6.0.3",
    "@types/unzipper": "^0.10.11",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.7",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
